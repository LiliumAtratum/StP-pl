    # voice "audio/voices/ch2/tower/_cont/hero/9.flac"
    # hero "Is... is she talking about you?\n"
    voice "audio/voices/ch2/tower/_cont/hero/9.flac"
    hero "Czy... czy ona mówi o tobie?\n"

# game/scripts/paths/tower/tower_1/tower_1_convo.rpy:317
